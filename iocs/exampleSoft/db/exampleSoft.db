record(ai, "exampleSoft:FD_CNT") {
  field(DESC, "Allocated File Descriptors")
  field(SCAN, "I/O Intr")
  field(DTYP, "IOC stats")
  field(FLNK, "exampleSoft:FD_FREE  PP MS")
  field(INP, "@fd")
}

record(calc, "exampleSoft:FD_FREE") {
  field(DESC, "Available FDs")
  field(CALC, "B>0?B-A:C")
  field(INPA, "exampleSoft:FD_CNT  NPP MS")
  field(INPB, "exampleSoft:FD_MAX  NPP MS")
  field(INPC, "1000")
  field(HOPR, "150")
  field(LOLO, "5")
  field(LOW, "20")
  field(LLSV, "MAJOR")
  field(LSV, "MINOR")
}

