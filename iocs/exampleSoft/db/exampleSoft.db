



#% macro, __doc__, Create some records for reading IOC statistics and details.
# Linux and VxWorks support
#% macro, name, Object name and GUI association name
#% macro, IOCNAME, Device prefix

# % gui, $(name), edm, ioc_stats.edl, ioc=exampleSoft
# % gui, $(name), edmembed, ioc_embed.edl, ioc=exampleSoft

# Used by Channel Access Security to determine access to this IOC.
record(mbbo, "exampleSoft:ACCESS")
{
  field(DESC, "exampleSoft Acc Mode")
  field(ASG,  "IOCMANAGERS")
  field(PINI, "YES")
  field(ZRST, "Running")
  field(ZRSV, "NO_ALARM")
  field(ONST, "Maintenance")
  field(ONSV, "MINOR")
  field(TWST, "Test")
  field(TWSV, "MINOR")
  field(THST, "OFFLINE")
  field(THSV, "MAJOR")
}
record(stringin, "exampleSoft:STARTTOD")
{
    field(DESC, "Time and date of startup")
    field(DTYP, "Soft Timestamp")
    field(PINI, "YES")
    field(INP, "@%m/%d/%Y %H:%M:%S")
}
record(stringin, "exampleSoft:TOD")
{
    field(DESC, "Current time and date")
    field(DTYP, "Soft Timestamp")
    field(SCAN, "1 second")
    field(INP, "@%m/%d/%Y %H:%M:%S")
}
record(calcout, "exampleSoft:HEARTBEAT")
{
    field(DESC, "1 Hz counter since boot")
    field(CALC, "(A<2147483647)?A+1:1")
    field(SCAN, "1 second")
    field(INPA, "exampleSoft:HEARTBEAT")
}
#
# Using an existing internal set of subroutines, this
# PV updates the Access Security mechanism dynamically.
# The .acf file is re-read.
#
record( sub, "exampleSoft:READACF")
{
    field( DESC, "exampleSoft ACF Update")
    field( ASG,  "IOCMANAGERS")
    field( INAM, "asSubInit")
    field( SNAM, "asSubProcess")
    field( BRSV, "INVALID")
}
record(sub, "exampleSoft:SYSRESET")
{
    field(DESC, "IOC Restart" )
    field(SNAM, "rebootProc")
    field(BRSV,"INVALID")
    field(L,"1")
}

